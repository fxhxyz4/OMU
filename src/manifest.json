{
	"manifest_version": 3,
	"name": "OMU",
	"description": "Open Multiple Urls",
	"version": "1.0.3",
	"author": "fxhxyz",
	"homepage_url": "https://omu-beta.vercel.app",
	"action": {
		"default_icon": {
			"16": "./img/icon.png",
			"32": "./img/icon.png",
			"48": "./img/icon.png",
			"128": "./img/icon.png"
		},
		"default_title": "OMU",
    "default_lang": "en",
		"default_popup": "./popup.html"
	},
	"icons": {
		"16": "./img/icon.png",
		"32": "./img/icon.png",
		"48": "./img/icon.png",
		"128": "./img/icon.png"
	},
  "permissions": [
    "tabs"
  ]
}
