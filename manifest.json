{
	"manifest_version": 3,
	"name": "OMU",
	"description": "Open Multiple Urls",
	"version": "1.0.2",
	"author": "fxhxyz",
	"homepage_url": "https://fxhxyz4.github.io/OMU/",
	"action": {
		"default_icon": {
			"16": "./src/img/icon.png",
			"32": "./src/img/icon.png",
			"48": "./src/img/icon.png",
			"128": "./src/img/icon.png"
		},
		"default_title": "OMU",
		"default_popup": "./src/popup.html"
	},
	"icons": {
		"16": "./src/img/icon.png",
		"32": "./src/img/icon.png",
		"48": "./src/img/icon.png",
		"128": "./src/img/icon.png"
	},
  "permissions": [
    "tabs"
  ]
}
